doctype html
html

  if url_prefix === undefined
    - url_prefix = ''

  head
    title= title

    meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='theme-color', content='#6a1b9a')

    link(rel='stylesheet', href='//fonts.googleapis.com/icon?family=Material+Icons')
    link(type='text/css', rel='stylesheet', href=url_prefix + '../stylesheets/vendor/materialize/materialize.css', media='screen,projection')
    link(type='text/css', rel='stylesheet', href=url_prefix + '../stylesheets/vendor/noUiSlider/nouislider.css', media='screen,projection')
    link(type='text/css', rel='stylesheet', href=url_prefix + '../stylesheets/vendor/prism/prism.css', media='screen,projection')
    link(type='text/css', rel='stylesheet', href=url_prefix + '../stylesheets/style.css', media='screen,projection')
  body
    block content
    script(type='text/javascript' src='https://code.jquery.com/jquery-2.2.4.min.js')
    script.
      if (!window.jQuery) {
        document.write('<script src="#{url_prefix}../js/vendor/jquery-2.2.4.min.js"><\/script>');
      }
    script(type='text/javascript' src=url_prefix + '../js/vendor/materialize/bin/materialize.min.js')
    script(type='text/javascript' src=url_prefix + '../js/vendor/noUiSlider/nouislider.min.js')
    script(type='text/javascript' src=url_prefix + '../js/vendor/validator/validator.min.js')
    script(type='text/javascript' src=url_prefix + '../js/vendor/progressbarjs/progressbar.min.js')
    script(type='text/javascript' src=url_prefix + '../js/vendor/prism/prism.min.js')
    script.
      window.url_prefix = '!{url_prefix}';
    block scripts
    block scripts2
