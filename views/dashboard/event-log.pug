extends ./index

block innerContent
  div#loader(style='z-index: 999999')
    .row
      .col.s12
        .valign-wrapper(style='height: 100vh')
          .valign.center(style='width: 100%')
            .preloader-wrapper.big.active
              .spinner-layer.spinner-blue
                .circle-clipper.left
                  .circle
                .gap-patch
                  .circle
                .circle-clipper.right
                  .circle
              .spinner-layer.spinner-red
                .circle-clipper.left
                  .circle
                .gap-patch
                  .circle
                .circle-clipper.right
                  .circle
              .spinner-layer.spinner-yellow
                .circle-clipper.left
                  .circle
                .gap-patch
                  .circle
                .circle-clipper.right
                  .circle
              .spinner-layer.spinner-green
                .circle-clipper.left
                  .circle
                .gap-patch
                  .circle
                .circle-clipper.right
                  .circle
  div#content
    .row
      .col.s10.offset-s1
        br
        h3.blue-grey-text Event Log

        ul.collapsible#eventlogCollection

        .row
          .col
            a.waves-effect.waves-light.btn(onclick='loadMore()') Load more
          .col.right#spinner
            .preloader-wrapper.small.active
              .spinner-layer.spinner-blue-only
                .circle-clipper.left
                  .circle
                .gap-patch
                  .circle
                .circle-clipper.right
                  .circle
block scripts2
  script(type='text/javascript' src=url_prefix + '../js/vendor/moment.js')
  script(type='text/javascript' src=url_prefix + '../js/vendor/moment-timezone.js')
  script(type='text/javascript' src=url_prefix + '../js/dashboard/dictionaries.js')
  script(type='text/javascript' src=url_prefix + '../js/dashboard/event-log.js')
